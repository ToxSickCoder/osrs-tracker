  "scripts": {
    "start": "ionic serve -- --aot",
    "start:devapp": "ionic serve --devapp -- --aot",
    "start:android": "npx cap sync && npx cap open android",
    "build": "ng build",
    "build:android": "npm run build && npx cap sync && npx cap open android",
    "build:android:prod": "npm run build:prod && npx cap sync && npx cap open android",
    "build:ci": "ng build --prod --progress false",
    "build:prod": "rm -rf www/ && ng build --prod",
    "docker:build": "docker build -t 'toxsickcoder/osrs-tracker-app:dev' .",
    "docker:deploy": "bash ./build/deploy.sh",
    "lint": "ng lint"
  },
  "private": true,
